{
    "name": "abdugoffor/laravel-crud-generator",
    "description": "A simple CRUD generator for Laravel models",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdugoffor",
            "email": "abdugofforqodirov19@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "DataTable\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DataTable\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
